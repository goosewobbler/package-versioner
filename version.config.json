{
  "$schema": "./package-versioner.schema.json",
  "versionPrefix": "v",
  "preset": "angular",
  "updateInternalDependencies": "patch",
  "commitMessage": "chore: release ${version} [skip-ci]",
  "updateChangelog": true,
  "tagTemplate": "${prefix}${version}",
  "packageSpecificTags": false,
  "cargo": {
    "enabled": true
  }
}
